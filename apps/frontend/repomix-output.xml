  ws.onclose = () => {
    setStatus('disconnected');
    wsRef.current = null;

    reconnectAttemptsRef.current++;
    const delay = Math.min(3000 * reconnectAttemptsRef.current, 30000); // Exponential backoff
    reconnectTimeoutRef.current = setTimeout(connect, delay);
